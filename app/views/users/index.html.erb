<h1 class="heading">users</h1>

<ul class="user-list">
  <% @users.each do |user| %>
    <li>      
      <% if user.user_image %>
      <%= image_tag "/user_images/#{user.user_image}", class: "profile_img" %>
    <% else %>
      <%= image_tag "images.jpg", class: "profile_img" %>
    <% end %>
      <%= link_to user.name, user %> |
      <%= user.email %>
    </li>
  <% end %>
</ul>


